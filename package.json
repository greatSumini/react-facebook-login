{
  "name": "@greatsumini/react-facebook-login",
  "version": "2.0.0",
  "keywords": [
    "react",
    "reactjs",
    "facebook",
    "oauth",
    "typescript"
  ],
  "description": "Well-typed React Component for Facebook Login",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.{ts,tsx} --ignore-path ./.prettierignore",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "prepublish:npm": "npm run format && npm run lint && npm run test && npm run build",
    "publish:npm": "npm run prepublish:npm && npm publish --public",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook -p 3000"
  },
  "author": "greatSumini <greatSumini@gmail.com> (https://sumini.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greatSumini/react-facebook-login.git"
  },
  "bugs": {
    "url": "https://github.com/greatSumini/react-facebook-login/issues"
  },
  "homepage": "https://sumini.dev/react-facebook-login",
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storysource": "^6.3.4",
    "@storybook/addons": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
}
